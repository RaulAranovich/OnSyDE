#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/owl/owlapi/turtle#compDtr
:compDtr rdf:type owl:ObjectProperty ;
         rdfs:domain :Phrase ;
         rdfs:range :Phrase .


###  http://www.semanticweb.org/owl/owlapi/turtle#headDtr
:headDtr rdf:type owl:ObjectProperty ;
         rdfs:domain :Phrase ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( :Phrase
                                    :Word
                                  )
                    ] .


###  http://www.semanticweb.org/owl/owlapi/turtle#object
:object rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#specDtr
:specDtr rdf:type owl:ObjectProperty ;
         rdfs:domain :Phrase ;
         rdfs:range :Phrase .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/owl/owlapi/turtle#AdjPhrase
:AdjPhrase rdf:type owl:Class ;
           rdfs:subClassOf :Phrase .


###  http://www.semanticweb.org/owl/owlapi/turtle#Adjective
:Adjective rdf:type owl:Class ;
           rdfs:subClassOf :Word .


###  http://www.semanticweb.org/owl/owlapi/turtle#DetPhrase
:DetPhrase rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                        owl:onProperty :compDtr ;
                                                                        owl:someValuesFrom :NounPhrase
                                                                      ]
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty :headDtr ;
                                                                        owl:someValuesFrom :Determiner
                                                                      ]
                                                                    ) ;
                                                 rdf:type owl:Class
                                               ]
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :headDtr ;
                                                 owl:someValuesFrom :ProperName
                                               ]
                                             )
                               ] ;
           rdfs:subClassOf :Phrase .


###  http://www.semanticweb.org/owl/owlapi/turtle#Determiner
:Determiner rdf:type owl:Class ;
            rdfs:subClassOf :Word .


###  http://www.semanticweb.org/owl/owlapi/turtle#IntransitiveVerb
:IntransitiveVerb rdf:type owl:Class ;
                  rdfs:subClassOf :Verb .


###  http://www.semanticweb.org/owl/owlapi/turtle#Noun
:Noun rdf:type owl:Class ;
      rdfs:subClassOf :Word .


###  http://www.semanticweb.org/owl/owlapi/turtle#NounPhrase
:NounPhrase rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :headDtr ;
                                  owl:someValuesFrom :Noun
                                ] ;
            rdfs:subClassOf :Phrase .


###  http://www.semanticweb.org/owl/owlapi/turtle#Phrase
:Phrase rdf:type owl:Class ;
        owl:disjointWith :Word .


###  http://www.semanticweb.org/owl/owlapi/turtle#PrepPhrase
:PrepPhrase rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                         owl:onProperty :compDtr ;
                                                         owl:someValuesFrom :DetPhrase
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :headDtr ;
                                                         owl:someValuesFrom :Preposition
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf :Phrase .


###  http://www.semanticweb.org/owl/owlapi/turtle#PrepVerb
:PrepVerb rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :object ;
                                owl:someValuesFrom :PrepPhrase
                              ] ;
          rdfs:subClassOf :Verb .


###  http://www.semanticweb.org/owl/owlapi/turtle#Preposition
:Preposition rdf:type owl:Class ;
             rdfs:subClassOf :Word .


###  http://www.semanticweb.org/owl/owlapi/turtle#ProperName
:ProperName rdf:type owl:Class ;
            rdfs:subClassOf :Word .


###  http://www.semanticweb.org/owl/owlapi/turtle#Sentence
:Sentence rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                       owl:onProperty :headDtr ;
                                                       owl:someValuesFrom :VerbPhrase
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :specDtr ;
                                                       owl:someValuesFrom :DetPhrase
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :Phrase .


###  http://www.semanticweb.org/owl/owlapi/turtle#TransitiveVerb
:TransitiveVerb rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :object ;
                                      owl:someValuesFrom :DetPhrase
                                    ] ;
                rdfs:subClassOf :Verb .


###  http://www.semanticweb.org/owl/owlapi/turtle#Verb
:Verb rdf:type owl:Class ;
      rdfs:subClassOf :Word ;
      owl:disjointUnionOf ( :IntransitiveVerb
                            :PrepVerb
                            :TransitiveVerb
                          ) .


###  http://www.semanticweb.org/owl/owlapi/turtle#VerbPhrase
:VerbPhrase rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                         owl:onProperty :compDtr ;
                                                                         owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                              owl:unionOf ( :AdjPhrase
                                                                                                            :DetPhrase
                                                                                                            :PrepPhrase
                                                                                                          )
                                                                                            ]
                                                                       ]
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty :headDtr ;
                                                                         owl:someValuesFrom :Verb
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :headDtr ;
                                                  owl:someValuesFrom :Verb
                                                ]
                                              )
                                ] ;
            rdfs:subClassOf :Phrase .


###  http://www.semanticweb.org/owl/owlapi/turtle#Word
:Word rdf:type owl:Class .


